;; PROBLEMS: 05
;; DESC: Reverse a list.
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
(defun my-reverse (lst)
  (if (null lst)
    ()
    (append (my-reverse (cdr lst)) (list (car lst)))))
